class Solution {
    public void rotate(int[] nums, int k) {
        int n= nums.length;
        k %= n;
        r(nums, 0, n-1);
        r(nums, 0, k-1);
        r(nums, k, n-1);
    }
 void r(int[] nums, int i, int j){
    while(i<j){
        int t=nums[i];
        nums[i]=nums[j];
        nums[j]=t;
        i++;
        j--;
    }
 }
}
